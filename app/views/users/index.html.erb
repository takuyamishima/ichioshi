<% breadcrumb :users %>
<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <%= user.email %>
      <%= link_to 'ユーザーを編集する', edit_user_registration_path  %>
      <!-- < %= link_to 'ユーザーを削除する', users_destroy_path(id: current_user.id), method: :delete ,data: { confirm: '本当に削除していいですか？' } %> -->
    </li>
  <% end %>
</ul>
