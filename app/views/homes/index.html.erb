<% @review.each do |review| %>
<td><%= link_to review.title , review_path(review.id) %></td>
<td><%= link_to image_tag(review.image.url), review_path(review.id) %></td>
<% end %>

<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
    <% @users.each do |user| %>
        <li>
            <%= link_to user.name, user %>
        </li>
    <% end %>
</ul>